function countdown(countdownId, timeId, serverStart, serverEnd, =
clientStart, replace) {
	var countEl =3D document.getElementById(countdownId);
	var tmEl =3D document.getElementById(timeId);
	if (countEl =3D=3D null && tmEl =3D=3D null)
		return;

	var elapsed =3D (new Date()).valueOf() - clientStart;
	var serverCurrentDate =3D new Date(serverStart + elapsed);
	var timeLeft =3D serverEnd - serverStart - elapsed + 5;

	if (countEl !=3D null) {
		if (timeLeft <=3D 10) {
			countEl.innerHTML =3D replace;
			var txtEl =3D document.getElementById(countdownId + "Text");
			if (txtEl !=3D null) {
				txtEl.innerHTML =3D "";
			}
		} else {
			countEl.innerHTML =3D TimeLeftToStr(timeLeft);
		}
	}
	if (tmEl !=3D null)
		tmEl.innerHTML =3D DateToStr(serverCurrentDate);

	var delay =3D 1001 - new Date(serverCurrentDate).getMilliseconds();
	setTimeout(function() {
		countdown(countdownId, timeId, serverStart, serverEnd, clientStart, =
replace);
	}, delay);
}

function DateToStr(date) {
	return format(date.getUTCDate(), 2) + "." + format(date.getUTCMonth() + =
1, 2) + "." + date.getUTCFullYear() + " "
		+ format(date.getUTCHours(), 2) + ":" + format(date.getUTCMinutes(), =
2) + ":" + format(date.getUTCSeconds(), 2);
}

function TimeLeftToStr(tm) {
	tm =3D Math.floor(tm / 1000);
	var sec =3D tm % 60;
	tm =3D Math.floor(tm / 60);
	var min =3D tm % 60;
	tm =3D Math.floor(tm / 60);
	return tm + ":" + format(min, 2) + ":" + format(sec, 2);
}

function format(d, len) {
	var str =3D d.toString();
	while (str.length < len)
		str =3D "0" + str;
	return str;
}

